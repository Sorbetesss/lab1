{
    "name": "symfony/emoji-transliterator",
    "type": "library",
    "description": "Provides a transliterator to convert emojis to an ASCII representation",
    "keywords": ["emoji", "transliteration"],
    "homepage": "https://symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "symfony/finder": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": { "Symfony\\Component\\EmojiTransliterator\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
