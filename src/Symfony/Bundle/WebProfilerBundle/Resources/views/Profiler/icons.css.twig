@font-face {
    font-family: 'entypo';
    src: url('{{ asset('bundles/webprofiler/entypo.eot') }}');
    src: url('{{ asset('bundles/webprofiler/entypo.eot') }}?#iefix') format('embedded-opentype'),
         url('{{ asset('bundles/webprofiler/entypo.woff') }}') format('woff'),
         url('{{ asset('bundles/webprofiler/entypo.ttf') }}') format('truetype'),
         url('{{ asset('bundles/webprofiler/entypo.svg') }}#entypo') format('svg');
    font-weight: normal;
    font-style: normal;
}

.icon-font {
    font-family: "entypo";
    font-size: 58px;
    width: 40px;
    display: inline-block;
    text-align: center;
    line-height: 28px;
}

