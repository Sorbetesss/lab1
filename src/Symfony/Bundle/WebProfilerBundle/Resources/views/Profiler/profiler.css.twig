#header h1 svg { width: 40px; height: 40px !important; position: relative; top: 10px; margin-right: 0px; }
#header h1 { color: #FFF; font-weight: normal; font-size: 25px; line-height: 40px; margin-top: -10px;}
#header h1 span { color: #CCC; }

#menu-profiler li.selected a,
#menu-profiler:hover li.selected a:hover,
