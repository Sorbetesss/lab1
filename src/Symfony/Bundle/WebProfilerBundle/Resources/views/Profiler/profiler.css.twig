body {
    background-color: #F9F9F9;
    color: #222;
    font-family: Helvetica, Arial, sans-serif;
    font-size: 62.5%;
    line-height: 1.4;
}

h2, h3 {
    font-weight: 500;
    margin: 1.5em 0 .5em;
}
h2 + h3 {
    margin-top: 1em;
}
h2 {
    font-size: 24px;
}
h3 {
    font-size: 21px;
}
li {
    margin-bottom: 10px;
}
p {
    font-size: 16px;
    margin-bottom: 1em
}
a {
    color: #218BC3;
    text-decoration: none;
}
a:hover {
    text-decoration: underline;
}
a:active,
a:hover {
    outline: 0;
}
img {
    border: 0;
}
h2 span, h3 span,
h2 small, h3 small {
    color: #999;
}
h2 a,
h3 a {
    text-decoration: underline;
}
h2 a:hover,
h3 a:hover {
    text-decoration: none;
}

button {
    font-family: "Helvetica Neue", Arial, sans-serif;
}
.btn {
    background: #777;
    border-radius: 2px;
    border: 0;
    color: #F5F5F5;
    display: inline-block;
    font-size: 14px;
    font-weight: normal;
    padding: .5em .75em;
}
.btn:hover {
    cursor: pointer;
    opacity: 0.8;
    text-decoration: none;
}
.btn.btn-primary {
    background: rgb(117, 158, 43);
    color: #FFF;
}
.btn.btn-sm {
    font-size: 12px;
}
.btn.btn-sm svg {
    height: 16px;
    vertical-align: middle;
}

caption {
    margin-bottom: 7px;
}
table, tr, th, td {
    background: #FFF;
    border-collapse: collapse;
    border: 1px solid #E0E0E0;
    border-width: 1px 0;
    font-size: 14px;
    line-height: 1.5;
    vertical-align: top;
}
table {
    border-width: 1px;
    margin: 1em 0;
    table-layout: fixed;
    width: 100%;
}
table.auto-layout {
    table-layout: auto;
}
table th {
    font-weight: bold;
    text-align: left;
}
table thead th {
    background-color: #E0E0E0;
    border-bottom: 1px solid #FAFAFA;
}
table thead th.key {
    width: 19%;
}
table tbody th {
    bbackground-color: #FFF;
}
table th, table td {
    padding: 8px 10px;
}
table td em {
    color: #aaa;
}

fieldset {
    border: none;
}
abbr {
    border-bottom: 1px dotted #000;
    cursor: help;
}
pre, code {
    font-size: 0.9em;
}

.help {
    color: #999;
    font-size: 14px;
}
p.help {
    margin-bottom: .5em;
}
.empty {
    border: 4px dashed #E0E0E0;
    color: #999;
    margin: 1em 0;
    padding: .5em 2em;
    text-align: left;
}

.muted {
    color: #999;
}
.nowrap {
    white-space: pre;
}
.newline {
    display: block;
}
.break-long-words {
    -ms-word-break: break-all;
     word-break: break-all;
     word-break: break-word;
-webkit-hyphens: auto;
   -moz-hyphens: auto;
        hyphens: auto;
}
.small-text {
    font-size: 12px;
}
.bold {
    font-weight: bold;
}
.text-right {
    text-align: right;
}

.metric {
    background: #FFF;
    border: 1px solid #E0E0E0;
    font-size: 16px;
    min-width: 100px;
    min-height: 70px;
}
.metric .value {
    display: block;
    font-size: 28px;
    padding: 5px 15px;
    text-align: center;
}
.metric .value svg {
    margin: 5px 0 -5px;
}
.metric .unit {
    color: #999;
    font-size: 18px;
    margin-left: -4px;
}
.metric .label {
    background: #E0E0E0;
    display: block;
    font-size: 12px;
    font-weight: bold;
    padding: 5px;
    text-align: center;
}
.metrics {
    margin: 1em 0;
    overflow: auto;
}
.metrics .metric {
    float: left;
    margin-right: 1em;
}

.metrics-horizontal .metric {
    min-height: auto;
    min-width: auto;
}
.metrics-horizontal .metric .value,
.metrics-horizontal .metric .label {
    display: inline;
    padding: 2px 6px;
}
.metrics-horizontal .metric .label {
    display: inline-block;
    padding: 6px;
}
.metrics-horizontal .metric .value {
    font-size: 16px;
}
.metrics-horizontal .metric .value svg {
    max-height: 14px;
    line-height: 10px;
    margin: 0;
    padding-left: 4px;
    vertical-align: middle;
}

table, .metric {
    box-shadow: 0px 0px 1px rgba(128, 128, 128, .2);
}

.container {
    min-width: 960px;
    max-width: 1200px;
    padding-right: 15px;
    margin-left: auto;
    margin-right: auto;
}
#header {
    background-color: #222;
    overflow: hidden;
}
#header h1 {
    float: left;
    margin: 0;
    padding: 10px;
}
#header h1 svg {
    height: 40px;
}
#header h1 svg path {
    fill: #FFF;
}
#header .search {
    float: right;
    padding-top: 15px;
}
#header .search input {
    border: 1px solid #dadada;
    font-size: 14px;
    margin-right: 4px;
    padding: 6px 8px;
    width: 195px;
}

#summary {
    z-index: 2000;
}
#summary .status {
    background: #E0E0E0;
    border: solid rgba(0, 0, 0, 0.2);
    border-width: 2px 0;
    padding: 10px;
    padding: 10px;
}
#summary .status-undefined {
    border-color: rgba(0, 0, 0, 0.1);
}
#summary .status-ok { background: #5E976E; }
#summary .status-warning { background: #BC8034; }
#summary .status-error { background: #B0413E; }

#summary .lead,
#summary #summary-url,
#summary #summary-url a {
    color: #FFF;
    font-size: 21px;
    margin: 0;
    text-decoration: none;
}
#summary #summary-url a:hover {
    text-decoration: underline;
}
#summary .status-undefined .lead {
    color: #222;
    font-size: 24px;
    margin: 8px 0;
}
#summary dl.metadata {
    margin: 5px 0 0;
    color: rgba(255, 255, 255, 0.7);
}
#summary dl.metadata dt,
#summary dl.metadata dd {
    display: inline-block;
    font-size: 13px;
}
#summary dl.metadata dt {
    font-weight: bold;
}
#summary dl.metadata dt:after {
    content: ":";
}
#summary dl.metadata dd {
    margin: 0 1.5em 0 0;
}

#sidebar {
    background: #444;
    color: #CCC;
    float: left;
    margin-left: -100%;
    width: 250px;

    margin-bottom: -99999px;
    padding-bottom: 99999px;
}
#sidebar a {
    color: #99CDD8;
}
#sidebar .module {
    padding: 10px;
}
#sidebar .module h3 {
    color: #999;
    font-size: 12px;
    font-weight: bold;
    text-transform: uppercase;
    margin-bottom: 10px;
}

#sidebar #sidebar-shortcuts {
    background: #333;
    padding: 16px 10px;
    /* needed to prevent visual issues dut to the pure CSS slideToggle() */
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}
#sidebar #sidebar-shortcuts .btn {
    color: #F5F5F5;
    margin-right: 10px;
}
#sidebar #sidebar-shortcuts #reveal-search-form {
    display: none;
}
#sidebar #sidebar-shortcuts #sidebar-search {
    height: 0;
    overflow: hidden;
    -moz-transition: height .25s ease;
    -webkit-transition: height .25s ease;
    transition: height .25s ease;
}
body#search #sidebar #sidebar-shortcuts #sidebar-search,
#sidebar #sidebar-shortcuts #reveal-search-form:checked + #sidebar-search {
    height: 290px;
}
body#search h3 {
    margin-top: 0;
}

#sidebar-search .form-group:first-of-type {
    padding-top: 20px;
}
#sidebar-search .form-group {
    clear: both;
    overflow: hidden;
    padding-bottom: 10px;
}
#sidebar-search .form-group label {
    float: left;
    font-size: 13px;
    line-height: 24px;
    width: 60px;
}
#sidebar-search .form-group input,
#sidebar-search .form-group select {
    float: left;
    font-size: 13px;
    padding: 3px 6px;
}
#sidebar-search .form-group input {
    background: #CCC;
    border: 1px solid #999;
    color: #222;
    width: 150px;
}
#sidebar-search .form-group select {
    color: #222;
}
#sidebar-search .form-group .btn {
    float: right;
    margin-right: 10px;
}
#sidebar-admin a {
    font-size: 14px;
    margin-left: 10px;
}

#menu-profiler {
    margin: 0;
    padding: 0;
}
#menu-profiler li {
    position: relative;
    margin-bottom: 0;
}
#menu-profiler li:last-child {
    bborder: 0;
}
#menu-profiler li a {
    border: solid transparent;
    border-width: 2px 0;
    color: #CCC;
    display: block;
    font-size: 13px;
    text-decoration: none;
    cursor: pointer;
}
#menu-profiler li a .label {
    color: #EEE;
    display: block;
    padding: 8px 10px 8px 60px;
    overflow: hidden;
    white-space: nowrap;
}
#menu-profiler li a .label .icon {
    display: block;
    position: absolute;
    left: 0;
    top: 8px;
    width: 60px;
    text-align: center;
}
#menu-profiler li a .label strong {
    font-size: 16px;
    font-weight: normal;
}
#menu-profiler li a .label.disabled {
    opacity: .25;
}
#menu-profiler li a:hover .label.disabled,
#menu-profiler li.selected a .label.disabled {
    opacity: 1;
}
