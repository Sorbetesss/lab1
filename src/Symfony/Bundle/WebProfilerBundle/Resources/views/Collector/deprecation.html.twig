{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block head %}
    <style type="text/css">
        {% include '@WebProfiler/Collector/exception.css.twig' %}
    </style>
    {{ parent() }}
{% endblock %}

{% block toolbar %}
    {% if collector.count > 0 %}
        {% set icon %}
            <span>
                <img width="16" height="28" alt="Deprecated methods used" src="data:image/png;base64," />
                <span class="sf-toolbar-status sf-toolbar-status-yellow">{{ collector.count }}</span>
            </span>
        {% endset %}
        {% set text %}
        <div class="sf-toolbar-info-piece">
            <b style="width: 170px;">Deprecated Methods used</b>
            <span>{{ collector.count }}</span>
        </div>
        {% endset %}
        {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': false } %}
    {% endif %}
{% endblock %}

