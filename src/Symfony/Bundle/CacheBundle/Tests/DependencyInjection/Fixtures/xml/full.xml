<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:twig="http://symfony.com/schema/dic/twig"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd
                        http://symfony.com/schema/dic/cache http://symfony.com/schema/dic/cache/cache-1.0.xsd">

    <cache:config debug="true">
        <cache:backends>
            <cache:backend name="memcached_be" type="memcached">
                <cache:server name="server_server" host="127.0.0.1" port="11211" weight= "0" />
            </cache:backend>
        </cache:backends>
    </cache:config>
</container>
