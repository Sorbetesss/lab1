{
    "name": "symphony/framework-bundle",
    "type": "symphony-bundle",
    "description": "Symphony FrameworkBundle",
    "keywords": [],
    "homepage": "https://symphony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symphony.com"
        },
        {
            "name": "Symphony Community",
            "homepage": "https://symphony.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-xml": "*",
        "symphony/cache": "~3.4|~4.0",
        "symphony/dependency-injection": "^4.1",
        "symphony/config": "~3.4|~4.0",
        "symphony/event-dispatcher": "^4.1",
        "symphony/http-foundation": "^4.1",
        "symphony/http-kernel": "^4.1",
        "symphony/polyfill-mbstring": "~1.0",
        "symphony/filesystem": "~3.4|~4.0",
        "symphony/finder": "~3.4|~4.0",
        "symphony/routing": "^4.1"
    },
    "require-dev": {
        "doctrine/cache": "~1.0",
        "fig/link-util": "^1.0",
        "symphony/asset": "~3.4|~4.0",
        "symphony/browser-kit": "~3.4|~4.0",
        "symphony/console": "~3.4|~4.0",
        "symphony/css-selector": "~3.4|~4.0",
        "symphony/dom-crawler": "~3.4|~4.0",
        "symphony/polyfill-intl-icu": "~1.0",
        "symphony/security": "~3.4|~4.0",
        "symphony/form": "^4.1",
        "symphony/expression-language": "~3.4|~4.0",
        "symphony/process": "~3.4|~4.0",
        "symphony/security-core": "~3.4|~4.0",
        "symphony/security-csrf": "~3.4|~4.0",
        "symphony/serializer": "^4.1",
        "symphony/stopwatch": "~3.4|~4.0",
        "symphony/translation": "~3.4|~4.0",
        "symphony/templating": "~3.4|~4.0",
        "symphony/validator": "^4.1",
        "symphony/var-dumper": "~3.4|~4.0",
        "symphony/workflow": "~3.4|~4.0",
        "symphony/yaml": "~3.4|~4.0",
        "symphony/property-info": "~3.4|~4.0",
        "symphony/lock": "~3.4|~4.0",
        "symphony/web-link": "~3.4|~4.0",
        "doctrine/annotations": "~1.0",
        "phpdocumentor/reflection-docblock": "^3.0|^4.0",
        "twig/twig": "~1.34|~2.4"
    },
    "conflict": {
        "phpdocumentor/reflection-docblock": "<3.0",
        "phpdocumentor/type-resolver": "<0.2.1",
        "phpunit/phpunit": "<4.8.35|<5.4.3,>=5.0",
        "symphony/asset": "<3.4",
        "symphony/console": "<3.4",
        "symphony/form": "<4.1",
        "symphony/property-info": "<3.4",
        "symphony/serializer": "<4.1",
        "symphony/stopwatch": "<3.4",
        "symphony/translation": "<3.4",
        "symphony/validator": "<4.1",
        "symphony/workflow": "<3.4"
    },
    "suggest": {
        "ext-apcu": "For best performance of the system caches",
        "symphony/console": "For using the console commands",
        "symphony/form": "For using forms",
        "symphony/serializer": "For using the serializer service",
        "symphony/validator": "For using validation",
        "symphony/yaml": "For using the debug:config and lint:yaml commands",
        "symphony/property-info": "For using the property_info service",
        "symphony/web-link": "For using web links, features such as preloading, prefetching or prerendering"
    },
    "autoload": {
        "psr-4": { "Symphony\\Bundle\\FrameworkBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
