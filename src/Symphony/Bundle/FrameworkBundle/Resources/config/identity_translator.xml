<?xml version="1.0" ?>
<container xmlns="http://symphony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symphony.com/schema/dic/services http://symphony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults public="false" />

        <service id="translator" class="Symphony\Component\Translation\IdentityTranslator" public="true">
            <argument type="service" id="translator.selector" />
        </service>
        <service id="Symphony\Component\Translation\TranslatorInterface" alias="translator" />

        <service id="translator.selector" class="Symphony\Component\Translation\MessageSelector" />
    </services>
</container>
