{
    "name": "symphony/web-server-bundle",
    "type": "symphony-bundle",
    "description": "Symphony WebServerBundle",
    "keywords": [],
    "homepage": "https://symphony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symphony.com"
        },
        {
            "name": "Symphony Community",
            "homepage": "https://symphony.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symphony/config": "~3.4|~4.0",
        "symphony/console": "~3.4|~4.0",
        "symphony/dependency-injection": "~3.4|~4.0",
        "symphony/http-kernel": "~3.4|~4.0",
        "symphony/process": "^3.4.2|^4.0.2"
    },
    "autoload": {
        "psr-4": { "Symphony\\Bundle\\WebServerBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "suggest": {
        "symphony/monolog-bridge": "For using the log server.",
        "symphony/expression-language": "For using the filter option of the log server."
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
