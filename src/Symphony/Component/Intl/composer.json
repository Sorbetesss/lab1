{
    "name": "symphony/intl",
    "type": "library",
    "description": "A PHP replacement layer for the C intl extension that includes additional data from the ICU library.",
    "keywords": ["intl", "icu", "internationalization", "localization", "i18n", "l10n"],
    "homepage": "https://symphony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernhard Schussek",
            "email": "bschussek@gmail.com"
        },
        {
            "name": "Eriksen Costa",
            "email": "eriksen.costa@infranology.com.br"
        },
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        },
        {
            "name": "Symphony Community",
            "homepage": "https://symphony.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symphony/polyfill-intl-icu": "~1.0"
    },
    "require-dev": {
        "symphony/filesystem": "~3.4|~4.0"
    },
    "suggest": {
        "ext-intl": "to use the component with locales other than \"en\""
    },
    "autoload": {
        "psr-4": { "Symphony\\Component\\Intl\\": "" },
        "classmap": [ "Resources/stubs" ],
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
