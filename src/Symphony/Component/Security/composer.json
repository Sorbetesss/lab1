{
    "name": "symphony/security",
    "type": "library",
    "description": "Symphony Security Component",
    "keywords": [],
    "homepage": "https://symphony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symphony.com"
        },
        {
            "name": "Symphony Community",
            "homepage": "https://symphony.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symphony/event-dispatcher": "~3.4|~4.0",
        "symphony/http-foundation": "~3.4|~4.0",
        "symphony/http-kernel": "~3.4|~4.0",
        "symphony/property-access": "~3.4|~4.0"
    },
    "replace": {
        "symphony/security-core": "self.version",
        "symphony/security-csrf": "self.version",
        "symphony/security-guard": "self.version",
        "symphony/security-http": "self.version"
    },
    "require-dev": {
        "psr/container": "^1.0",
        "symphony/finder": "~3.4|~4.0",
        "symphony/polyfill-intl-icu": "~1.0",
        "symphony/routing": "~3.4|~4.0",
        "symphony/validator": "~3.4|~4.0",
        "symphony/expression-language": "~3.4|~4.0",
        "symphony/ldap": "~3.4|~4.0",
        "psr/log": "~1.0"
    },
    "suggest": {
        "psr/container": "To instantiate the Security class",
        "symphony/form": "",
        "symphony/validator": "For using the user password constraint",
        "symphony/routing": "For using the HttpUtils class to create sub-requests, redirect the user, and match URLs",
        "symphony/expression-language": "For using the expression voter",
        "symphony/ldap": "For using the LDAP user and authentication providers"
    },
    "autoload": {
        "psr-4": { "Symphony\\Component\\Security\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
