{
    "name": "symphony/ldap",
    "type": "library",
    "description": "An abstraction in front of PHP's LDAP functions, compatible with PHP 5.3.9 onwards.",
    "keywords": ["ldap", "active directory"],
    "homepage": "https://symphony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles Sarrazin",
            "email": "charles@sarraz.in"
        },
        {
            "name": "Symphony Community",
            "homepage": "https://symphony.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symphony/options-resolver": "~3.4|~4.0",
        "ext-ldap": "*"
    },
    "autoload": {
        "psr-4": { "Symphony\\Component\\Ldap\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
