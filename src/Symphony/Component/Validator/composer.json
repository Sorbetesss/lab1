{
    "name": "symphony/validator",
    "type": "library",
    "description": "Symphony Validator Component",
    "keywords": [],
    "homepage": "https://symphony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symphony.com"
        },
        {
            "name": "Symphony Community",
            "homepage": "https://symphony.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symphony/polyfill-mbstring": "~1.0",
        "symphony/translation": "~3.4|~4.0"
    },
    "require-dev": {
        "symphony/http-foundation": "~4.1",
        "symphony/http-kernel": "~3.4|~4.0",
        "symphony/var-dumper": "~3.4|~4.0",
        "symphony/intl": "~4.1",
        "symphony/yaml": "~3.4|~4.0",
        "symphony/config": "~3.4|~4.0",
        "symphony/dependency-injection": "~3.4|~4.0",
        "symphony/expression-language": "~3.4|~4.0",
        "symphony/cache": "~3.4|~4.0",
        "symphony/property-access": "~3.4|~4.0",
        "doctrine/annotations": "~1.0",
        "doctrine/cache": "~1.0",
        "egulias/email-validator": "^1.2.8|~2.0"
    },
    "conflict": {
        "phpunit/phpunit": "<4.8.35|<5.4.3,>=5.0",
        "symphony/dependency-injection": "<3.4",
        "symphony/http-kernel": "<3.4",
        "symphony/intl": "<4.1",
        "symphony/yaml": "<3.4"
    },
    "suggest": {
        "psr/cache-implementation": "For using the metadata cache.",
        "doctrine/annotations": "For using the annotation mapping. You will also need doctrine/cache.",
        "doctrine/cache": "For using the default cached annotation reader and metadata cache.",
        "symphony/http-foundation": "",
        "symphony/intl": "",
        "symphony/yaml": "",
        "symphony/config": "",
        "egulias/email-validator": "Strict (RFC compliant) email validation",
        "symphony/property-access": "For accessing properties within comparison constraints",
        "symphony/expression-language": "For using the Expression validator"
    },
    "autoload": {
        "psr-4": { "Symphony\\Component\\Validator\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
