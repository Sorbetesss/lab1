{
    "name": "symphony/var-dumper",
    "type": "library",
    "description": "Symphony mechanism for exploring and dumping PHP variables",
    "keywords": ["dump", "debug"],
    "homepage": "https://symphony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Grekas",
            "email": "p@tchwork.com"
        },
        {
            "name": "Symphony Community",
            "homepage": "https://symphony.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symphony/polyfill-mbstring": "~1.0",
        "symphony/polyfill-php72": "~1.5"
    },
    "require-dev": {
        "ext-iconv": "*",
        "symphony/process": "~3.4|~4.0",
        "twig/twig": "~1.34|~2.4"
    },
    "conflict": {
        "phpunit/phpunit": "<4.8.35|<5.4.3,>=5.0",
        "symphony/console": "<3.4"
    },
    "suggest": {
        "ext-iconv": "To convert non-UTF-8 strings to UTF-8 (or symphony/polyfill-iconv in case ext-iconv cannot be used).",
        "ext-intl": "To show region name in time zone dump",
        "symphony/console": "To use the ServerDumpCommand and/or the bin/var-dump-server script"
    },
    "autoload": {
        "files": [ "Resources/functions/dump.php" ],
        "psr-4": { "Symphony\\Component\\VarDumper\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "bin": [
        "Resources/bin/var-dump-server"
    ],
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
