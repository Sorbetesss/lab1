{
    "name": "symphony/dependency-injection",
    "type": "library",
    "description": "Symphony DependencyInjection Component",
    "keywords": [],
    "homepage": "https://symphony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symphony.com"
        },
        {
            "name": "Symphony Community",
            "homepage": "https://symphony.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "symphony/yaml": "~3.4|~4.0",
        "symphony/config": "~4.1",
        "symphony/expression-language": "~3.4|~4.0"
    },
    "suggest": {
        "symphony/yaml": "",
        "symphony/config": "",
        "symphony/finder": "For using double-star glob patterns or when GLOB_BRACE portability is required",
        "symphony/expression-language": "For using expressions in service container configuration",
        "symphony/proxy-manager-bridge": "Generate service proxies to lazy load them"
    },
    "conflict": {
        "symphony/config": "<4.1",
        "symphony/finder": "<3.4",
        "symphony/proxy-manager-bridge": "<3.4",
        "symphony/yaml": "<3.4"
    },
    "provide": {
        "psr/container-implementation": "1.0"
    },
    "autoload": {
        "psr-4": { "Symphony\\Component\\DependencyInjection\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
