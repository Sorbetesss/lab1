{
    "name": "symphony/doctrine-bridge",
    "type": "symphony-bridge",
    "description": "Symphony Doctrine Bridge",
    "keywords": [],
    "homepage": "https://symphony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symphony.com"
        },
        {
            "name": "Symphony Community",
            "homepage": "https://symphony.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/common": "~2.4",
        "symphony/polyfill-mbstring": "~1.0"
    },
    "require-dev": {
        "symphony/stopwatch": "~3.4|~4.0",
        "symphony/dependency-injection": "~3.4|~4.0",
        "symphony/form": "~3.4|~4.0",
        "symphony/http-kernel": "~3.4|~4.0",
        "symphony/property-access": "~3.4|~4.0",
        "symphony/property-info": "~3.4|~4.0",
        "symphony/proxy-manager-bridge": "~3.4|~4.0",
        "symphony/security": "~3.4|~4.0",
        "symphony/expression-language": "~3.4|~4.0",
        "symphony/validator": "~3.4|~4.0",
        "symphony/translation": "~3.4|~4.0",
        "doctrine/data-fixtures": "1.0.*",
        "doctrine/dbal": "~2.4",
        "doctrine/orm": "^2.4.5"
    },
    "conflict": {
        "phpunit/phpunit": "<4.8.35|<5.4.3,>=5.0",
        "symphony/dependency-injection": "<3.4"
    },
    "suggest": {
        "symphony/form": "",
        "symphony/validator": "",
        "symphony/property-info": "",
        "doctrine/data-fixtures": "",
        "doctrine/dbal": "",
        "doctrine/orm": ""
    },
    "autoload": {
        "psr-4": { "Symphony\\Bridge\\Doctrine\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
