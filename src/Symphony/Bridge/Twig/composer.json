{
    "name": "symphony/twig-bridge",
    "type": "symphony-bridge",
    "description": "Symphony Twig Bridge",
    "keywords": [],
    "homepage": "https://symphony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symphony.com"
        },
        {
            "name": "Symphony Community",
            "homepage": "https://symphony.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "twig/twig": "^1.35|^2.4.4"
    },
    "require-dev": {
        "symphony/asset": "~3.4|~4.0",
        "symphony/dependency-injection": "~3.4|~4.0",
        "symphony/finder": "~3.4|~4.0",
        "symphony/form": "^4.1",
        "symphony/http-foundation": "~3.4|~4.0",
        "symphony/http-kernel": "~3.4|~4.0",
        "symphony/polyfill-intl-icu": "~1.0",
        "symphony/routing": "~3.4|~4.0",
        "symphony/templating": "~3.4|~4.0",
        "symphony/translation": "~3.4|~4.0",
        "symphony/yaml": "~3.4|~4.0",
        "symphony/security": "~3.4|~4.0",
        "symphony/security-acl": "~2.8|~3.0",
        "symphony/stopwatch": "~3.4|~4.0",
        "symphony/console": "~3.4|~4.0",
        "symphony/var-dumper": "~3.4|~4.0",
        "symphony/expression-language": "~3.4|~4.0",
        "symphony/web-link": "~3.4|~4.0",
        "symphony/workflow": "~3.4|~4.0"
    },
    "conflict": {
        "symphony/form": "<4.1",
        "symphony/console": "<3.4"
    },
    "suggest": {
        "symphony/finder": "",
        "symphony/asset": "For using the AssetExtension",
        "symphony/form": "For using the FormExtension",
        "symphony/http-kernel": "For using the HttpKernelExtension",
        "symphony/routing": "For using the RoutingExtension",
        "symphony/templating": "For using the TwigEngine",
        "symphony/translation": "For using the TranslationExtension",
        "symphony/yaml": "For using the YamlExtension",
        "symphony/security": "For using the SecurityExtension",
        "symphony/stopwatch": "For using the StopwatchExtension",
        "symphony/var-dumper": "For using the DumpExtension",
        "symphony/expression-language": "For using the ExpressionExtension",
        "symphony/web-link": "For using the WebLinkExtension"
    },
    "autoload": {
        "psr-4": { "Symphony\\Bridge\\Twig\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
