{
    "name": "symphony/symphony",
    "type": "library",
    "description": "The Symphony PHP framework",
    "keywords": ["framework"],
    "homepage": "https://symphony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symphony.com"
        },
        {
            "name": "Symphony Community",
            "homepage": "https://symphony.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-xml": "*",
        "doctrine/common": "~2.4",
        "fig/link-util": "^1.0",
        "twig/twig": "^1.35|^2.4.4",
        "psr/cache": "~1.0",
        "psr/container": "^1.0",
        "psr/link": "^1.0",
        "psr/log": "~1.0",
        "psr/simple-cache": "^1.0",
        "symphony/polyfill-intl-icu": "~1.0",
        "symphony/polyfill-mbstring": "~1.0",
        "symphony/polyfill-php72": "~1.5"
    },
    "replace": {
        "symphony/asset": "self.version",
        "symphony/browser-kit": "self.version",
        "symphony/cache": "self.version",
        "symphony/config": "self.version",
        "symphony/console": "self.version",
        "symphony/css-selector": "self.version",
        "symphony/dependency-injection": "self.version",
        "symphony/debug": "self.version",
        "symphony/debug-bundle": "self.version",
        "symphony/doctrine-bridge": "self.version",
        "symphony/dom-crawler": "self.version",
        "symphony/dotenv": "self.version",
        "symphony/event-dispatcher": "self.version",
        "symphony/expression-language": "self.version",
        "symphony/filesystem": "self.version",
        "symphony/finder": "self.version",
        "symphony/form": "self.version",
        "symphony/framework-bundle": "self.version",
        "symphony/http-foundation": "self.version",
        "symphony/http-kernel": "self.version",
        "symphony/inflector": "self.version",
        "symphony/intl": "self.version",
        "symphony/ldap": "self.version",
        "symphony/lock": "self.version",
        "symphony/messenger": "self.version",
        "symphony/monolog-bridge": "self.version",
        "symphony/options-resolver": "self.version",
        "symphony/process": "self.version",
        "symphony/property-access": "self.version",
        "symphony/property-info": "self.version",
        "symphony/proxy-manager-bridge": "self.version",
        "symphony/routing": "self.version",
        "symphony/security": "self.version",
        "symphony/security-core": "self.version",
        "symphony/security-csrf": "self.version",
        "symphony/security-guard": "self.version",
        "symphony/security-http": "self.version",
        "symphony/security-bundle": "self.version",
        "symphony/serializer": "self.version",
        "symphony/stopwatch": "self.version",
        "symphony/templating": "self.version",
        "symphony/translation": "self.version",
        "symphony/twig-bridge": "self.version",
        "symphony/twig-bundle": "self.version",
        "symphony/validator": "self.version",
        "symphony/var-dumper": "self.version",
        "symphony/web-link": "self.version",
        "symphony/web-profiler-bundle": "self.version",
        "symphony/web-server-bundle": "self.version",
        "symphony/workflow": "self.version",
        "symphony/yaml": "self.version"
    },
    "require-dev": {
        "cache/integration-tests": "dev-master",
        "doctrine/annotations": "~1.0",
        "doctrine/cache": "~1.6",
        "doctrine/data-fixtures": "1.0.*",
        "doctrine/dbal": "~2.4",
        "doctrine/orm": "~2.4,>=2.4.5",
        "doctrine/doctrine-bundle": "~1.4",
        "monolog/monolog": "~1.11",
        "ocramius/proxy-manager": "~0.4|~1.0|~2.0",
        "predis/predis": "~1.0",
        "egulias/email-validator": "~1.2,>=1.2.8|~2.0",
        "symphony/phpunit-bridge": "~3.4|~4.0",
        "symphony/security-acl": "~2.8|~3.0",
        "phpdocumentor/reflection-docblock": "^3.0|^4.0"
    },
    "conflict": {
        "phpdocumentor/reflection-docblock": "<3.0||>=3.2.0,<3.2.2",
        "phpdocumentor/type-resolver": "<0.2.1",
        "phpunit/phpunit": "<5.4.3"
    },
    "provide": {
        "psr/cache-implementation": "1.0",
        "psr/container-implementation": "1.0",
        "psr/log-implementation": "1.0",
        "psr/simple-cache-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Symphony\\Bridge\\Doctrine\\": "src/Symphony/Bridge/Doctrine/",
            "Symphony\\Bridge\\Monolog\\": "src/Symphony/Bridge/Monolog/",
            "Symphony\\Bridge\\ProxyManager\\": "src/Symphony/Bridge/ProxyManager/",
            "Symphony\\Bridge\\Twig\\": "src/Symphony/Bridge/Twig/",
            "Symphony\\Bundle\\": "src/Symphony/Bundle/",
            "Symphony\\Component\\": "src/Symphony/Component/"
        },
        "classmap": [
            "src/Symphony/Component/Intl/Resources/stubs"
        ],
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "autoload-dev": {
        "files": [ "src/Symphony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
